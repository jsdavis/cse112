{
  "name": "cse112",
  "version": "1.0.0",
  "description": "Unlucky Geniuses' CSE 112 project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint --fix ./*.js public/js/*.js",
    "test": "mocha",
    "doc": "jsdoc -d ./docs ./*.js public/js/*.js",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "postinstall": "node nightwatch.conf.js"  
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdavis/cse112.git"
  },
  "author": "Unlucky Geniuses",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsdavis/cse112/issues"
  },
  "homepage": "https://github.com/jsdavis/cse112#readme",
  "dependencies": {
    "express": "^4.15.2",
    "mocha": "^3.2.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.10",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "git-hooks": "^1.1.8",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.15",
    "selenium-download": "^2.0.10"
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "ecmaFeatures": {
      "destructuring": true
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "varsIgnorePattern": "Ignored"
        }
      ]
    }
  }
}
