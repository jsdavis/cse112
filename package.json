{
  "name": "cse112",
  "version": "1.0.0",
  "description": "Unlucky Geniuses' CSE 112 project",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development node index.js | bunyan -o short",
    "lint": "eslint --fix .",
    "test": "NODE_ENV=test mocha",
    "doc": "jsdoc -d ./docs -c jsdoc.json",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "clean": "rm -rf public/js-es5",
    "build": "npm run clean && mkdir public/js-es5 && babel public/js -s -d public/js-es5",
    "production": "npm run build && node bin/production",
    "postinstall": "node nightwatch.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdavis/cse112.git"
  },
  "author": "Unlucky Geniuses",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsdavis/cse112/issues"
  },
  "homepage": "https://github.com/jsdavis/cse112#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "bunyan": "^1.8.10",
    "express": "^4.15.2",
    "mocha": "^3.2.0",
    "mongoose": "^4.9.9",
    "mongoose-encryption": "^1.5.0",
    "selenium-download": "^2.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "git-hooks": "^1.1.8",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.15"
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "ecmaFeatures": {
      "destructuring": true
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "varsIgnorePattern": "Ignored"
        }
      ]
    }
  }
}
